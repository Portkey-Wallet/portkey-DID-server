{
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "127.0.0.1",
        "Port": "5672"
      }
    },
    "EventBus": {
      "ClientName": "ContractEventHandler",
      "ExchangeName": "Exchange"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning",
        "Quartz": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "retainedFileCountLimit": 15
        }
      }
    ]
  },
  "Chains": {
    "ChainInfos": {
      "AELF": {
        "ChainId": "AELF",
        "BaseUrl": "http://127.0.0.1:8000",
        "ContractAddress": "xxx",
        "PrivateKey": "xxx",
        "IsMainChain": true
      },
      "tDVV": {
        "ChainId": "tDVV",
        "BaseUrl": "http://127.0.0.1:8000",
        "ContractAddress": "xxx",
        "PrivateKey": "xxx",
        "IsMainChain": false
      }
    }
  },
  "Index": {
    "IndexDelay": 5000,
    "IndexInterval": 300,
    "IndexSafe": 100
  },
  "GraphQL": {
    "GraphQLConnection": "http://127.0.0.1:8083/AElfIndexer_DApp/PortKeyIndexerCASchema/graphql"
  },
  "Sync": {
    "Cron": "0 */3 * * * ?"
  },
  "Orleans": {
    "ClusterId": "CAServerSiloCluster",
    "ServiceId": "CAServerOrleansBasicService",
    "MongoDBClient": "mongodb://localhost:27017/?maxPoolSize=555",
    "DataBase": "CAServerOrleansDB"
  }
}