{
  "App": {
    "CorsOrigins": "https://*.CAServer.com,http://localhost:4200"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:5577"
      }
    }
  },
  "ConnectionStrings": {
    "Default": "mongodb://localhost:27017/CAServer"
  },
  "Redis": {
    "Configuration": "127.0.0.1"
  },
  "AuthServer": {
    "Authority": "http://localhost:8080",
    "RequireHttpsMetadata": "false",
    "SwaggerClientId": "CAServer_App"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "xxx"
  },                                                           
  "Orleans": {
    "ClusterId": "CAServerSiloCluster",
    "ServiceId": "CAServerOrleansBasicService",
    "MongoDBClient": "mongodb://localhost:27017/?maxPoolSize=555",
    "DataBase": "CAServerOrleansDB"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "127.0.0.1",
        "Port": "5672"
      }
    },
    "EventBus": {
      "ClientName": "Api",
      "ExchangeName": "Exchange"
    }
  },
  "CoinGecko": {
    "CoinIdMapping": {
      "ELF": "aelf"
    }
  },
  "ElasticUris": {
    "Uris": [
      "http://127.0.0.1:9200"
    ]
  },
  "IndexSetting": {
    "NumberOfShards": 5,
    "NumberOfReplicas": 1,
    "IndexPrefix": "CAServer"
  },
  "GraphQL": {
    "Configuration": "http://127.0.0.1:8083/AElfIndexer_DApp/PortKeyIndexerCASchema/graphql"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "retainedFileCountLimit": 10
        }
      }
    ]
  },
  "Hub": {
    "Configuration": {
      "MethodResponseTtl": {
        "Method1": 240
      },
      "DefaultResponseTtl": 300
    }
  },
  "Chains": {
    "ChainInfos": {
      "AELF": {
        "ChainId": "AELF",
        "BaseUrl": "http://127.0.0.1:8000",
        "ContractAddress": "xxx",
        "PrivateKey": "xxx"
      },
      "tDVV": {
        "ChainId": "tDVV",
        "BaseUrl": "http://127.0.0.1:8000",
        "ContractAddress": "xxx",
        "PrivateKey": "xxx"
      }
    }
  },
  "AdaptableVariableSetting": {
    "VerifierServerExpireTime": 15,
    "HttpConnectTimeOut": 5
  }
}
