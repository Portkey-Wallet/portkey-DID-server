{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning",
        "Quartz": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "retainedFileCountLimit": 15
        }
      }
    ]
  },
  "Chains": {
    "ChainInfos": {
      "AELF": {
        "ChainId": "AELF",
        "BaseUrl": "127.0.0.1",
        "ContractAddress": "xxx",
        "TokenContractAddress": "xxx",
        "CrossChainContractAddress": "xxx",
        "RedPackageContractAddress": "xxx",
        "PublicKey": "xxx",
        "IsMainChain": true
      },
      "tDVW": {
        "ChainId": "tDVW",
        "BaseUrl": "127.0.0.1",
        "ContractAddress": "xxx",
        "TokenContractAddress": "xxx",
        "CrossChainContractAddress": "xxx",
        "RedPackageContractAddress": "xxx",
        "PublicKey": "xxx",
        "IsMainChain": false
      }
    }
  },
  "Orleans": {
    "ClusterId": "CAServerSiloCluster",
    "ServiceId": "CAServerOrleansBasicService",
    "MongoDBClient": "mongodb://127.0.0.1:27017/?maxPoolSize=555",
    "DataBase": "CAServerOrleansDB",
    "ResponseTimeout": 60
  },
  "Redis": {
    "Configuration": "127.0.0.1"
  },
  "SignatureServer": {
    "BaseUrl": "http://127.0.0.1:18080/api/app/signature"
  },
  "ContractOptions": {
    "CommonPrivateKeyForCallTx": "xxx"
  },
  "ElasticUris": {
    "Uris": [
      "http://127.0.0.1:9200"
    ]
  },
  "IndexSetting": {
    "NumberOfShards": 5,
    "NumberOfReplicas": 1,
    "IndexPrefix": "CAServer"
  },
  "RedPackagePayAccount": {
    "RedPackagePayAccounts": [
      "***"
    ]
  },
  "GraphQL": {
    "Configuration": "http://127.0.0.1:8080/Portkey_V2_DID/PortKeyIndexerCASchema/graphql"
  }
}